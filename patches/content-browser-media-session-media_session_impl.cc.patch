+void MediaSessionImpl::GetLoadedUrl(
+    base::OnceCallback<void(const GURL&, bool)> callback) {
+  if (normal_players_.size() != 1) {
+    return std::move(callback).Run({}, {});
+  }
+
+  normal_players_.begin()->first.observer->OnGetLoadedUrl(
+      normal_players_.begin()->first.player_id, std::move(callback));
+}
+
+std::optional<MediaPlayerId> MediaSessionImpl::GetActivePlayerId() const {
+  if (normal_players_.size() != 1) {
+    return std::nullopt;
+  }
+
+  return normal_players_.begin()->first.observer->GetMediaPlayerId();
+}
